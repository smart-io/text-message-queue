{
  "name": "smart/textmessagequeue",
  "type": "library",
  "description": "Text message queue powered by Gearman",
  "keywords": [
    "Text Message",
    "Queue",
    "Gearman"
  ],
  "homepage": "https://github.com/smart-io/text-message-queue",
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Gilbert",
      "email": "kevin@kegisiteweb.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/console": "~2",
    "doctrine/orm": "~2",
    "cocur/slugify": "~1",
    "sinergi/gearman": "<1",
    "sinergi/token": "<1",
    "psr/log": "~1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "suggest": {
    "ext-gearman": "This library needs Gearman"
  },
  "autoload": {
    "psr-4": {
      "Smart\\TextMessageQueue\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\TextMessageQueue\\Tests\\": ["tests/TextMessageQueue/Tests"]
    }
  }
}
